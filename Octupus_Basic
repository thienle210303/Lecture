import java.util.Scanner;
public class Octupus {

	public static void main(String[] args) {
		Scanner keyboard = new Scanner(System.in);
		
		System.out.println("I'm an octupus, so I like the number 8. \nEnter a positive value and I'll count up by 8's! Blub blub");
		int numberInput = keyboard.nextInt();
		
		if(numberInput<0)
		{
			System.out.println("That's not valid!");
			System.exit(0);
		}
		
		for(int i=0;i<numberInput;i+=8)
		{
			System.out.print(i);
		}
		
		keyboard.close();
    }
}
